{
  "name": "Skeletor",
  "author": "Delphic Digital",
  "version": "0.16.0",
  "scripts": {
    "start": "gulp",
    "test": "ava --tap | tap-spec",
    "build": "gulp build"
  },
  "ava": {
    "files": [
      "./src/js/**/*.test.js"
    ],
    "source": [
      "**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": "babel-core/register",
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "bourbon": "^4.3.4",
    "breakpoint": "^0.1.6",
    "breakpoint-sass": "^2.7.0",
    "browser-sync": "^2.18.13",
    "cssnano": "^3.10.0",
    "eslint": "^4.6.1",
    "eslint-loader": "^1.9.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-image-resize": "^0.13.0",
    "gulp-inline-svg": "^1.1.1",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-svg-sprite": "^1.3.7",
    "gulp-svgmin": "^1.2.4",
    "gulp.spritesmith": "^6.5.1",
    "import-glob-loader": "^1.1.0",
    "node-bourbon": "^4.2.8",
    "node-sass": "^4.5.3",
    "node-sass-globbing": "0.0.23",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.0.2",
    "sass-lint": "^1.12.0",
    "susy": "^3.0.1",
    "svgo": "^0.7.2",
    "tap-diff": "^0.1.1",
    "tap-spec": "^4.1.1",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-spritesmith": "^0.3.3",
    "webpack-stream": "^4.0.0"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "picturefill": "^3.0.2",
    "svg4everybody": "^2.1.9",
    "velocity-animate": "^1.5.0"
  }
}
