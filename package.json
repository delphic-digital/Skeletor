{
  "name": "skeletor",
  "version": "3.0.0",
  "description": "Extensible front-end framework",
  "main": "index.js",
  "jspm": {
    "name": "skeletor",
    "main": "main.js",
    "dependencies": {
      "jquery": "npm:jquery@^3.1.1"
    },
    "devDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.16"
    },
    "overrides": {
      "npm:jquery@3.1.1": {
        "format": "amd"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delphic-digital/Skeletor.git"
  },
  "keywords": [
    "skeletor",
    "delphic"
  ],
  "author": "delphic",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/delphic-digital/Skeletor/issues"
  },
  "homepage": "http://skele.to/r",
  "devDependencies": {
    "jspm": "^0.17.0-beta.29",
    "npm-run-all": "^3.1.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel server watch:js",
    "server": "browser-sync start --server --files Static/dist/**/* --no-open",
    "watch:js": "jspm bundle Static/src/js/main.js Static/dist/js/main.js -wid"
  }
}
